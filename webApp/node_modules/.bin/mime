#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/webApp/node_modules/mime/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/webApp/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/node_modules:/mnt/c/Users/angel/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/webApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/webApp/node_modules/mime/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/webApp/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/node_modules:/mnt/c/Users/angel/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/angel/OneDrive - Instituto Politecnico Nacional/ESCOM/TT/Protocolo TT/SuiteSenior/webApp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
